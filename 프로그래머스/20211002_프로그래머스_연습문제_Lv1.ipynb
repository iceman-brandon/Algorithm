      "source": [
        "# 가운데 글자 가져오기 문제\n",
        "def solution(s):\n",
        "    if len(s) % 2 == 1: # string 문자의 길이가 홀수인 경우\n",
        "        return s[len(s)//2]\n",
        "    else:\n",
        "        return s[(len(s)//2-1) : (len(s)//2+1)]"
      ],
      "execution_count": 1,
      "outputs": []
    },
